# ============================================
# LLM API配置
# ============================================
API_KEY=your_api_key_here
# API_BASE需要指向兼容OpenAI的DashScope接口
API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1
MODEL_NAME=qwen-max

# ============================================
# 日志配置
# ============================================
# 日志级别: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# 日志输出路径
LOG_DIR=./workspace

# ============================================
# 实验配置
# ============================================
# 随机种子（用于结果复现）
RANDOM_SEED=42

# 最大并发Agent数量
MAX_CONCURRENT_AGENTS=5

# Agent超时时间（秒）
AGENT_TIMEOUT=300

# Agent数目
AGENT_NUM=30

# Agent配置文件
AGENT_CONFIG_DIR=./core/config/agent.json

# ============================================
# MLE-bench配置
# ============================================

# 当前使用的竞赛名称
MLE_BENCH_COMPETITION=aerial-cactus-identification

# MLE-bench工作空间目录（用于存放竞赛工作文件）
MLE_BENCH_WORKSPACE_DIR=./workspace

# 最大运行时间限制（小时）（用于替换additional_notes.txt中的{time_limit}）
MLE_BENCH_TIME_LIMIT=4

# 最大步骤数限制（用于替换additional_notes.txt中的{step_limit}）
MLE_BENCH_STEP_LIMIT=100

# Agent使用的Conda环境名称
CONDA_ENV_NAME=agent

# 评分服务器访问地址
MLE_BENCH_SERVER_URL=http://127.0.0.1:8001

# 私有基准数据存放目录
MLE_BENCH_PRIVATE_DATA_DIR=./dataset/private

# ============================================
# 任务执行配置
# ============================================
# 初始任务数量
INIT_TASK_NUM=4

# Explore任务比例 (0.0 - 1.0)
EXPLORE_RATIO=0.5

# 每次添加的任务数量
STEP_TASK_NUM=2